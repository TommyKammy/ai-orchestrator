    {
      "parameters": {
        "jsCode": "const d = $json;\n\n// Support both response shapes (direct or wrapped in body)\nconst msg = d.payload ? d : (d.body && d.body.payload ? d.body : d);\n\n// Get headers from correct location\nconst headers = msg.payload?.headers || [];\nconst get = (name) =>\n  headers.find(h => (h.name || '').toLowerCase() === name.toLowerCase())?.value;\n\nconst from = get('From') || 'Unknown';\nconst subject = get('Subject') || 'Unknown';\nconst date = get('Date') || 'Unknown';\n\nconst internalDate = Number(msg.internalDate || 0);\nconst id = msg.id || 'unknown';\n\n// Read cursor from Load Cursor node (via cross-node reference)\nlet lastSeen = 0;\ntry {\n  const cursorData = $('Load Cursor').first().json;\n  lastSeen = Number(cursorData.last_seen_internal_date || 0);\n} catch(e) {\n  lastSeen = 0;\n}\n\n// Dedupe check\nif (internalDate && internalDate <= lastSeen) {\n  return [{ json: { status: \"SKIP_OLD\", id } }];\n}\n\nreturn [{\n  json: {\n    status: \"NEW\",\n    id,\n    threadId: msg.threadId,\n    from,\n    subject,\n    date,\n    internalDate,\n    link: `https://mail.google.com/mail/u/0/#inbox/${id}`\n  }\n}];"
      },
      "id": "code-dedupe-format-1",
      "name": "Dedupe + Format",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1450,
        200
      ]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "cond-1",
              "leftValue": "={{ $json.status }}",
              "rightValue": "NEW",
              "operator": {
                "type": "string",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        }
      },
      "id": "if-is-new-1",
      "name": "Is New?",
